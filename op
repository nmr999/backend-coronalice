public class Operator {
public static void main(String[] args) {
System.out.println(10 > 20);
System.out.println( (10 > 20) || (20 < 30) );

int x = 10; x += 10; System.out.println(x); int num1 = 10; int num2; num2 = ++num1; System.out.println(num2 + "," + num1); num2 = num1++; System.out.println(num2 + "," + num1); System.out.println( (10>20) ? "greater" : "lesser"); }
}
